{
  "emeraldwalk.runonsave": {
    "commands": [
        {
            "match": "\\.dart$",
            "isAsync": true,
            "cmd": "bash ${workspaceFolder}/scripts/run_on_save_take_screenshot.sh ${fileBasename}"
            //  "echo '>> dart ${workspaceFolder} | ${workspaceFolder} | ${file} | ${fileBasename} | ${fileDirname}'"
        },
    ]
  },
  "dart.lineLength": 120,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    // "editor.renderWhitespace": "all",
  },
  "[go]": {
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  }
  // "go.toolsGopath": ""
}